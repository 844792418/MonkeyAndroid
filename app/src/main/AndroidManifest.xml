<?xml version="1.0" encoding="utf-8"?>
<manifest
  package="com.sunjiajia.monkeyandroid"
  xmlns:android="http://schemas.android.com/apk/res/android"
  >

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    >
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".LinearLayoutActivity"></activity>
    <activity android:name=".RelativeTableLayoutActivity"></activity>
    <activity android:name=".FrameGridLayoutActivity"></activity>
    <activity android:name=".ToastActivity"></activity>
    <activity android:name=".TextViewEditTextActivity"></activity>
  </application>

</manifest>
